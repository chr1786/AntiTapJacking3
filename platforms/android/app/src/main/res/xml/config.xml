<?xml version='1.0' encoding='utf-8'?>
<widget id="com.andy.antitapjacking3" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="TapjackingProtectionPlugin">
        <param name="android-package" value="com.bedefended.tapjackingprotection.TapjackingProtectionPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="packagemanager">
        <param name="android-package" value="fc.fcstudio.packagemanager" />
    </feature>
    <feature name="ExitApp">
        <param name="android-package" value="org.apache.cordova.exitapp.ExitApp" />
    </feature>
    <name>AntiTapJacking3</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
